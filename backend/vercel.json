{
    "version": 2,
    "buildCommand": "npm install && prisma generate",
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["src/**", "prisma/**"],
                "excludeFiles": ["node_modules/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}